{
  "name": "DigOut",
  "description": "Know you better than yourself",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/digout.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "hot-reload.js",
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}